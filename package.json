{
	"name": "dmarc-wiki",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
        "prepare": "svelte-kit sync",
		"build": "vite build",
		"postbuild": "npx svelte-sitemap --domain https://dmarc.wiki",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@ethercorps/sveltekit-og": "^4.0.0-next.1",
		"@kasisoft/remark-svelte-auto-import": "^0.2.7",
		"@sveltejs/adapter-static": "^3.0.9",
		"@sveltejs/enhanced-img": "^0.3.8",
		"@sveltejs/kit": "^2.36.2",
		"@sveltejs/vite-plugin-svelte": "^6.1.3",
		"@tailwindcss/typography": "^0.5.15",
		"@types/eslint": "^9.6.0",
		"@types/node": "^24.3.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.34.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.11.0",
		"globals": "^16.3.0",
		"mdsvex": "^0.11.2",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"rehype-rewrite": "^4.0.2",
		"svelte": "^5.38.3",
		"svelte-check": "^4.3.1",
		"svooltip": "^0.9.0",
		"tailwindcss": "^3.4.9",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.41.0",
		"vite": "^7.1.3"
	},
	"type": "module",
	"dependencies": {
		"@fontsource-variable/inter": "^5.2.6",
		"@fontsource/zilla-slab": "^5.2.6",
		"lucide-svelte": "^0.542.0",
		"mode-watcher": "^0.4.1",
		"svelte-sitemap": "^2.7.1"
	}
}
